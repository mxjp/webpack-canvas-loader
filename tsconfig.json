{
	"compilerOptions": {
		"declaration": true,
		"sourceMap": true,
		"module": "CommonJS",
		"moduleResolution": "Node",
		"target": "ES2020",
		"outDir": "dist",
		"rootDir": "src",
		"skipLibCheck": true,
		"esModuleInterop": true
	},
	"include": [
		"src"
	],
	"exclude": [
		"node_modules"
	]
}
