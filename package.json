{
	"name": "webpack-canvas-loader",
	"version": "1.0.0",
	"description": "Process files using the canvas api to cut, resize or apply filters on images.",
	"scripts": {
		"build": "tsc",
		"test": "rimraf dist/test && tsc -p tsconfig-test.json && ava -v",
		"start": "rimraf dist/test && tsc -p tsconfig-test.json && concurrently \"tsc -p tsconfig-test.json -w\" \"ava -w -v\"",
		"prepack": "npm run build"
	},
	"dependencies": {
		"canvas": "^2.5.0",
		"loader-utils": "^1.2.3"
	},
	"devDependencies": {
		"@types/loader-utils": "^1.1.3",
		"@types/memory-fs": "^0.3.2",
		"ava": "^2.2.0",
		"concurrently": "^4.1.1",
		"memory-fs": "^0.4.1",
		"typescript": "^3.5.3",
		"webpack": "^4.39.1"
	},
	"ava": {
		"files": [
			"./dist/test/test/**/*.js"
		]
	},
	"peerDependencies": {
		"webpack": "^4.0.0"
	},
	"main": "dist/index.js",
	"repository": "github:mxjp/webpack-canvas-loader",
	"author": "Max J. Polster",
	"license": "MIT"
}
